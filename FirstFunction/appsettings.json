{
    "Serilog": {
        "Using" : ["Serilog.Sinks.Console"],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessID", "WithThreadId" ],
        "WriteTo": [
            { "Name": "Console" }
        ],
        "Properties": {
            "Application": "AzFunctionSerilog"
        }
    } 
}